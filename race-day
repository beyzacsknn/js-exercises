let raceNumber = Math.floor(Math.random() * 1000);

let early=true;

let age=10;


if(early && age > 18)
{
  raceNumber=raceNumber+1000;
}

if(early && age>18)
{
  console.log('Early adults will race at 9:30. Your race number is: '+raceNumber);
}

else if (!early && age > 18)
{
  console.log('Late adults will race at 11:00am. Your race number is: '+raceNumber);
}

else if (age < 18)
{
  console.log('Youth registrants will race 12:30pm. Your race number is: '+raceNumber);
}
